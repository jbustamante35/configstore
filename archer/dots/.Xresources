!*.foreground:       #651009
!*.foreground_bold:  #ba9a6b
!*.background:       #2b2b2b
!*.cursor:           #2b2b2b
!*.color0:           #000000
!*.color1:           #cc241d
!*.color2:           #98971a
!*.color3:           #d79921
!*.color4:           #458588
