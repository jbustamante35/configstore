#!/bin/sh
# Set-up for VNC local forwarding

dsp="$1"
svr="$2"

case $svr in
    off)
        # Lab office desktop
        user=kinematics
        ;;

    *)
        # Default to JDev server
        user=jbustamante
        ;;
esac

# Setup VNC server on remote machine
#ssh -L "$dsp":localhost:"$dsp" -N -f -l "$user" "$svr" -p "$port"
ssh -L "$dsp":localhost:"$dsp" -N -f -l "$user" "$svr"
