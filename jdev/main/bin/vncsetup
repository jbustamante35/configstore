#!/bin/sh
# Set-up for VNC local forwarding

dsp="$1"
svr="$2"

case $svr in
    sub)
        # Spalding Lab Submit Machine
        ip=128.104.98.118
        port=22
        ;;

    dev)
        # JDev remote server
        ip=128.104.98.13
        port=22
        ;;

    *)
        # Default to JDev server
        ip=128.104.98.13
        port=22
        ;;
esac

# Setup VNC server on remote machine
ssh -L "$dsp":localhost:"$dsp" -N -f -l jbustamante "$ip" -p "$port"


